{
    "name": "Jatic",
    "description": "Jira automatic tasks and issues creator",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
        "default_title": "Let's create some task?",
        "default_icon": "Jira_32.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ]
}
